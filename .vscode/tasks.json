{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "Compile resources",
      "command": "rc.exe",
      "args": [
        "/foobj/stadia-vigem.res",
        "res/res.rc"
      ],
      "group": "build",
      "problemMatcher": [
        "$msCompile"
      ]
    },
    {
      "type": "shell",
      "label": "Build service (debug)",
      "command": "./Build-Debug.cmd",
      "problemMatcher": [
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "shell",
      "label": "Build service (release)",
      "command": "./Build-Release.cmd",
      "problemMatcher": [
        "$msCompile"
      ],
      "group": "build"
    }
  ]
}